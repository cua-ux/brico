                font-size: 1.1rem;
            }
            
            section {
                padding: 3rem 5%;
            }
            
            .section-title h2 {
                font-size: 2rem;
            }
            
            .btn-outline {
                margin-left: 0;
                margin-top: 15px;
            }
        }
